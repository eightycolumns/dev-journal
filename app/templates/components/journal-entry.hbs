<section class='journal-entry'>
  <h3>{{journal-entry.title}}</h3>
  <p class='date'>{{format-date-string journal-entry.date}}</p>
  <p class='mood'>Mood: {{journal-entry.mood}}</p>
  <p class='text'>{{journal-entry.text}}</p>
  <button class='delete' {{action 'delete' journal-entry}}>Delete</button>
</section>
